<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" style="-fx-background-color: #f8f9fa;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.stationeryshop.controller.inventory.InventoryController">
    <children>
        <Label style="-fx-font-weight: bold; -fx-font-size: 18px;" text="QUẢN LÝ TỒN KHO">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <TableView fx:id="inventoryTable" prefHeight="400.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="invIdColumn" prefWidth="95.0" resizable="false" text="ID" />
                <TableColumn fx:id="invProductColumn" prefWidth="239.0" resizable="false" text="Sản phẩm" />
                <TableColumn fx:id="invQuantityColumn" minWidth="0.0" prefWidth="235.0" resizable="false" text="Tồn kho" />
                <TableColumn fx:id="invLastStockedColumn" minWidth="-1.0" prefWidth="229.0" resizable="false" text="Ngày nhập" />
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
